<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.gui.ListBilletController">
   <children>
      <ImageView fitHeight="423.0" fitWidth="673.0" layoutX="-36.0" layoutY="-20.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/backg.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tabBillet" layoutX="30.0" layoutY="52.0" prefHeight="276.0" prefWidth="540.0" style="-fx-background-color: transparent; -fx-border-color: #ffffff;">
        <columns>
            <TableColumn fx:id="id" prefWidth="34.0" text="Id" />
            <TableColumn fx:id="terminal" prefWidth="75.0" text="terminal" />
            <TableColumn fx:id="portail" prefWidth="75.0" text="portail" />
            <TableColumn fx:id="embarquement" prefWidth="165.0" text="embarquement" />
            <TableColumn fx:id="localisation" prefWidth="89.0" text="localisation" />
            <TableColumn fx:id="chair_billet" prefWidth="48.0" text="Chair" />
            <TableColumn fx:id="voyage_num" prefWidth="48.0" text="Voyage" />
        </columns>
      </TableView>
      <Button fx:id="AjouterBillet" layoutX="474.0" layoutY="337.0" mnemonicParsing="false" onAction="#AjouterBillet" style="-fx-background-color: transparent; -fx-border-color: #ffffff; -fx-border-radius: 40%;" text="Ajouter" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="SupprimerBillet" layoutX="207.0" layoutY="337.0" mnemonicParsing="false" onAction="#SupprimerBillet" style="-fx-background-color: transparent; -fx-border-color: #ffffff; -fx-border-radius: 40%;" text="Supprimer" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="ModifierBillet" layoutX="90.0" layoutY="337.0" mnemonicParsing="false" onAction="#ModifierBillet" style="-fx-background-color: transparent; -fx-border-color: #ffffff; -fx-border-radius: 40%;" text="Modifier" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="ActualiserBillet" layoutX="334.0" layoutY="337.0" mnemonicParsing="false" onAction="#ActualiserBillet" style="-fx-background-color: transparent; -fx-border-color: #ffffff; -fx-border-radius: 40%;" text="Actualiser" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
